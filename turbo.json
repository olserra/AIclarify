{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [
    "NEXT_CI_RUNNER"
  ],
  "daemon": false,
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    }
  },
  "ui": "tui"
}